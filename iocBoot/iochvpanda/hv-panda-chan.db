
record(bo, "$(P)CH0:ENA") {
  field(ZNAM, "Disabled")
  field(ONAM, "Enabled")
  field(OUT, "$(P)CH0:ENA_")
  field(PINI, "YES")
}

record(longout, "$(P)CH0:ENA_") {
  field(DTYP, "Explore PCI Write32")
  field(OUT , "@$(DEV) 0 100 $(OFFSET)")
}

record(bi, "$(P)CH0:ENA_STS") {
  field(DTYP, "Explore PCI Read32")
  field(INP , "@$(DEV) 0 110 $(OFFSET)")
  field(MASK, "1")
  field(ZNAM, "Disabled")
  field(ONAM, "Enabled")  
  field(SCAN, ".1 second")
}

record(ai, "$(P)CH0:I") {
  field(DTYP, "Explore PCI Read32")
  field(INP , "@$(DEV) 0 10c $(OFFSET)")
  field(EGU , "A")
  field(SCAN, ".1 second")
}

record(ai, "$(P)CH0:V") {
  field(DTYP, "Explore PCI Read32")
  field(INP , "@$(DEV) 0 108 $(OFFSET)")
  field(ASLO, "0.1")
  field(EGU , "V")
  field(SCAN, ".1 second")
}

