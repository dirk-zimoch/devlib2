
record(bo, "$(P)ENA") {
  field(ZNAM, "Disabled")
  field(ONAM, "Enabled")
  field(OUT, "$(P)ENA_ PP")
  field(PINI, "YES")
}

record(longout, "$(P)ENA_") {
  field(DTYP, "Explore PCI Write32")
  field(OUT , "@$(DEV) 0 100 $(OFFSET)")
}

record(bi, "$(P)ENA_STS") {
  field(DTYP, "Explore PCI Read32")
  field(INP , "@$(DEV) 0 110 $(OFFSET)")
  field(MASK, "1")
  field(ZNAM, "Disabled")
  field(ONAM, "Enabled")  
  field(SCAN, ".1 second")
}

record(ai, "$(P)I") {
  field(DTYP, "Explore PCI Read32")
  field(INP , "@$(DEV) 0 10c $(OFFSET)")
  field(EGU , "A")
  field(SCAN, ".1 second")
}

record(ai, "$(P)V") {
  field(DTYP, "Explore PCI Read32")
  field(INP , "@$(DEV) 0 108 $(OFFSET)")
  field(ASLO, "0.1")
  field(EGU , "V")
  field(SCAN, ".1 second")
}

record(ao, "$(P)V_SET") {
  field(DTYP, "Explore PCI Write32")
  field(OUT , "@$(DEV) 0 104 $(OFFSET)")
  field(ASLO, "0.1")
  field(EGU , "V")
  field(PINI, "YES")
}

record(ao, "$(P)RUP_SET") {
  field(DTYP, "Explore PCI Write32")
  field(OUT , "@$(DEV) 0 114 $(OFFSET)")
  field(EGU , "V/s")
  field(RVAL , "500")
  field(PINI, "YES")
}

record(ao, "$(P)RDN_SET") {
  field(DTYP, "Explore PCI Write32")
  field(OUT , "@$(DEV) 0 118 $(OFFSET)")
  field(EGU , "V/s")
  field(RVAL , "500")
  field(PINI, "YES")
}
